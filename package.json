    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.0",
    "jest-each": "^24.7.0",
    "jest-fetch-mock": "^2.1.1",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-import": "^12.0.1",
