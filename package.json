{
  "name": "js-payments",
  "version": "2.0.0-beta.2",
  "description": "> A JavaScript interface for allowing tokenisation and authorisation of payments through SecureTrading.",
  "private": true,
  "browserslist": [
    "> 0.2% and last 2 versions",
    "not dead",
    "IE 10"
  ],
  "config": {
    "host": "localhost",
    "locale": "en_GB"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:automated": "webpack --config webpack.test.js",
    "start:automated": "webpack-dev-server --color --progress --config webpack.test.js",
    "start": "webpack-dev-server --color --progress --config webpack.dev.js",
    "example": "ws --rewrite '/* -> /dist/$1' --https",
    "prod": "npm run build",
    "test": "jest --watchAll",
    "coverage": "jest --coverage",
    "snyk": "snyk test --org=secure-trading",
    "testdocker": "docker pull \"securetrading1/js-payments-testing:$DOCKER_BRANCH\" && docker run -d -p 8443:8443 -p 8760:8760 -it \"securetrading1/js-payments-testing:$DOCKER_BRANCH\"",
    "behave": "npm run testdocker && cd js-payments-testing && make",
    "export:translations": "cross-env-shell i18next-conv -l '$npm_package_config_locale' -s ./src/core/translations/$npm_package_config_locale.json -t ./translations/$npm_package_config_locale.po",
    "import:translations": "cross-env-shell i18next-conv -l '$npm_package_config_locale' -s ./translations/$npm_package_config_locale.po -t ./src/core/translations/$npm_package_config_locale.json",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SecureTrading/js-payments.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SecureTrading/js-payments/issues"
  },
  "homepage": "https://github.com/SecureTrading/js-payments#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/plugin-transform-typescript": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.17",
    "@types/jest-each": "^0.3.4",
    "@types/jest-matchers": "^20.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "file-loader": "^4.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "i18next-conv": "^9.1.0",
    "jest": "^24.8.0",
    "jest-each": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-get-type": "^24.8.0",
    "live-server": "^1.2.1",
    "local-web-server": "^3.0.7",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "npm-merge-driver": "^2.3.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.0.0",
    "prettier": "1.18.2",
    "sass-loader": "^7.1.0",
    "snyk": "^1.216.0",
    "source-map-loader": "^0.2.4",
    "stylelint": "^10.1.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-loader": "^3.5.4",
    "typedoc": "^0.15.0",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.5.5",
    "@types/joi": "^14.3.3",
    "@types/jwt-decode": "^2.2.1",
    "i18next": "^17.0.9",
    "joi": "^14.3.1",
    "joi-browser": "^13.4.0",
    "jwt-decode": "^2.2.0",
    "location-origin": "^1.1.4",
    "ts-money": "^0.4.6",
    "url-polyfill": "^1.1.7",
    "validate": "^4.5.1",
    "whatwg-fetch": "^3.0.0"
  },
  "snyk": true
}
