{
  "name": "js-payments",
  "version": "2.0.9",
  "description": "> A JavaScript interface for allowing tokenisation and authorisation of payments through SecureTrading.",
  "private": true,
  "browserslist": [
    "> 0.2% and last 2 versions",
    "not dead",
    "IE 10"
  ],
  "config": {
    "host": "localhost",
    "locale": "en_GB"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:automated": "webpack --config webpack.test.js",
    "start:automated": "webpack-dev-server --color --progress --config webpack.test.js",
    "start": "webpack-dev-server --color --progress --config webpack.dev.js",
    "example": "ws --rewrite '/* -> /dist/$1' --https",
    "prod": "npm run build",
    "test": "jest --watchAll",
    "coverage": "jest --coverage",
    "snyk": "snyk test --org=secure-trading",
    "testdocker": "docker pull \"securetrading1/js-payments-testing:$DOCKER_BRANCH\" && docker run -d -p 8443:8443 -p 8760:8760 -it \"securetrading1/js-payments-testing:$DOCKER_BRANCH\"",
    "behave": "npm run testdocker && cd js-payments-testing &&",
    "export:translations": "cross-env-shell i18next-conv -l '$npm_package_config_locale' -s src/application/core/translations/$npm_package_config_locale.json -t src/translations/$npm_package_config_locale.po",
    "import:translations": "cross-env-shell i18next-conv -l '$npm_package_config_locale' -s src/translations/$npm_package_config_locale.po -t src/application/core/translations/$npm_package_config_locale.json",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "preinstall": "npx npm-force-resolutions",
    "prettier": "./node_modules/.bin/prettier --write ./src/**/*.{ts,js,css,json}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SecureTrading/js-payments.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SecureTrading/js-payments/issues"
  },
  "homepage": "https://github.com/SecureTrading/js-payments#readme",
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run prettier"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-catch-binding": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/plugin-transform-typescript": "^7.9.0",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "^25.1.3",
    "@types/jest-each": "^24.3.0",
    "@types/node": "13.9.2",
    "babel-jest": "^25.2.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "coveralls": "^3.0.11",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-loader": "^1.0.0",
    "html-webpack-plugin": "^4.0.3",
    "husky": "^4.2.3",
    "i18next-conv": "^9.1.0",
    "jest": "^25.2.4",
    "jest-each": "^25.2.3",
    "jest-fetch-mock": "^3.0.1",
    "jest-get-type": "^25.2.1",
    "lint-staged": "^10.0.10",
    "live-server": "^1.2.1",
    "local-web-server": "^4.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.0.0",
    "prettier": "2.0.2",
    "sass-loader": "^8.0.2",
    "snyk": "^1.305.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "stylelint-webpack-plugin": "^1.2.2",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.1",
    "ts-mockito": "^2.5.0",
    "tslint": "^6.1.1",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.8.3",
    "url-loader": "^4.0.0",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.2",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/runtime": "^7.8.4",
    "@hapi/joi": "^17.1.1",
    "@securetrading/js-payments-card": "^1.0.14",
    "@securetrading/ts-iin-lookup": "^0.0.6",
    "@securetrading/ts-luhn-check": "^0.0.5",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.149",
    "core-js": "^2.6.11",
    "fastestsmallesttextencoderdecoder": "^1.0.14",
    "i18next": "^19.3.4",
    "jwt-decode": "^2.2.0",
    "location-origin": "^1.1.4",
    "minimist": "^1.2.5",
    "npm-force-resolutions": "0.0.3",
    "promise-polyfill": "8.1.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "^6.5.4",
    "stylelint": "^13.2.1",
    "ts-money": "^0.4.6",
    "typedi": "^0.8.0",
    "url-polyfill": "^1.1.8",
    "validate": "^5.1.0",
    "whatwg-fetch": "^3.0.0"
  },
  "snyk": true
}
